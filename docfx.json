{
  "metadata": [
    {
      "src": [
        {
          "files": [ "./src/**.csproj","api/**.yml","api/**.md" ],
        }
      ],
      "dest": "docs/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**/**.yml",
          "**/**.md"
        ],
        "src": "docs",
        "dest": "."
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml",
        ],
        "src": "docs",
        "dest": "."
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "src": "docs",
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}